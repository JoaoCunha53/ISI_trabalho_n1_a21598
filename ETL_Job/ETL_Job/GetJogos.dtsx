        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{

            SmtpClient smtpClient = new SmtpClient();
            MailMessage email = new MailMessage();

            // Set SMTP client credentials and configuration
            smtpClient.UseDefaultCredentials = false;
            string smtpUsername = Dts.Variables["$Project::SMTPUsername"].Value.ToString();
            string smtpPassword = Dts.Variables["$Project::SMTPPassword"].Value.ToString();
            smtpClient.Credentials = new NetworkCredential(smtpUsername, smtpPassword);
            smtpClient.Port = Convert.ToInt32(Dts.Variables["$Project::SMTPPort"].Value);
            smtpClient.EnableSsl = true;
            smtpClient.Host = Dts.Variables["$Project::SMTPServer"].Value.ToString();

            // Set up the email message
            email.From = new MailAddress(Dts.Variables["$Project::SMTPUsername"].Value.ToString());
            email.To.Add(Dts.Variables["$Project::SMTPTo"].Value.ToString());
            email.Subject ="ERRO" + " - " + Dts.Variables["$System::PackageName"].Value.ToString();
            email.IsBodyHtml = false;
            email.Body = Dts.Variables["$System::ErrorDescription"].Value.ToString();

            // Send the email
            smtpClient.Send(email);


            Dts.TaskResult = (int)ScriptResults.Success;
        }

